<html t:type="layout" title="Upload zoek bestanden"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
	xmlns:p="tapestry:parameter">
<body>
	<p>Selecteer zoek afbeeldingen</p>
	<t:form t:id="uploadForm">
		<t:errors />
		<input t:type="upload" class="" t:id="file" t:value="file" validate="required" style="margin-bottom 10px; " />
		<input type="submit" value="Upload" />
	</t:form>
	<p>Zoek afbeeldingen:</p>
	<t:loop source="searchImages" value="searchImage">
		<p>
			<t:actionLink t:id="removeImage" context="searchImage"
				class="btn btn-primary btn-large">Verwijder afbeelding</t:actionLink>
			${searchImage}
		</p>
	</t:loop>
</body>
</html>
